{
	"expo": {
		"name": "GuardianG",
		"slug": "GuardianG",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/images/icon.png",
		"scheme": "guardiang",
		"userInterfaceStyle": "automatic",
		"newArchEnabled": true,
		"ios": {
			"bundleIdentifier": "com.faelayis.guardiang",
			"supportsTablet": true
		},
		"android": {
			"package": "com.faelayis.guardiang",
			"adaptiveIcon": {
				"foregroundImage": "./assets/images/adaptive-icon.png",
				"backgroundColor": "#ffffff"
			},
			"edgeToEdgeEnabled": true
		},
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "./assets/images/favicon.png"
		},
		"plugins": [
			"expo-router",
			"expo-dev-client",
			[
				"expo-splash-screen",
				{
					"image": "./assets/images/splash-icon.png",
					"imageWidth": 200,
					"resizeMode": "contain",
					"backgroundColor": "#ffffff"
				}
			]
		],
		"experiments": {
			"typedRoutes": true
		},
		"extra": {
			"router": {},
			"eas": {
				"projectId": "53d5f550-450d-4054-8c54-a8a45a86f747"
			}
		},
		"owner": "faelayis"
	}
}
